{
  "name": "@genki/folding-3d",
  "version": "0.1.0",
  "description": "3D折叠可视化组件库 - 基于React Three Fiber",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "three": ">=0.150.0",
    "@react-three/fiber": ">=8.0.0",
    "@react-three/drei": ">=9.0.0"
  },
  "dependencies": {
    "@genki/folding-engine": "workspace:*",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/three": "^0.182.0",
    "tsup": "^8.0.0",
    "typescript": "~5.9.3"
  }
}
